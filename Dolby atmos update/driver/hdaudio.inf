ServiceType   = 0x00000010
StartType     = 2
ErrorControl  = 1
ServiceBinary = %10%\system32\dolbyaposvc\DAX3API.exe

[DisableProtectedAudioDG.AddReg]
HKLM,"SOFTWARE\Microsoft\Windows\CurrentVersion\Audio","DisableProtectedAudioDG",0x00010001,0x1

