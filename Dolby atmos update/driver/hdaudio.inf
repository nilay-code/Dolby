[HdAud_Service_Install]
DisplayName   = "Service for High Definition Audio (WDM)"
ServiceType   = 1
StartType     = 3
ErrorControl  = 1
ServiceBinary = %10%\system32\drivers\HdAudio.sys

[DolbyAPO_ServiceInstall]
